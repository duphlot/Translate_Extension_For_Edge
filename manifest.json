{
  "manifest_version": 3,
  "name": "Translate Selection to Vietnamese",
  "version": "1.1",
  "description": "Dịch tự động khi bôi đen văn bản sang tiếng Việt",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icons/icon.png"
  }
}
